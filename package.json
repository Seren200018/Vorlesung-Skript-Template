{
  "name": "avc-script-template",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/avc-script-template.umd.js",
  "module": "./dist/avc-script-template.js",
  "exports": {
    ".": {
      "import": "./dist/avc-script-template.js",
      "require": "./dist/avc-script-template.umd.js",
      "default": "./dist/avc-script-template.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:pages": "vite build --config vite.pages.config.js && node tools/copy-pages-static.js",
    "build:template": "vite build --config vite.pages.template.config.js && node tools/copy-pages-static.js dist-template",
    "deploy:pages": "bash tools/deploy-site.sh pages",
    "preview": "vite preview",
    "build:offline": "node tools/offline-pack.js",
    "prepare": "npm run build"
  },
  "dependencies": {
    "animejs": "^3.2.2",
    "jsxgraph": "^1.10.1",
    "jszip": "^3.10.1",
    "mathjax": "^3.2.2",
    "mathjs": "^14.9.1",
    "matrixmath": "^2.2.2",
    "plotly": "^1.0.6",
    "plotly.js-dist": "^2.35.3",
    "plotly.js-dist-min": "^2.35.3",
    "react": "^19.0.0",
    "react-router": "^7.13.0",
    "react-router-dom": "^7.13.0",
    "roughjs": "^4.6.6",
    "vivus": "^0.4.6"
  },
  "devDependencies": {
    "vite": "^6.4.1"
  }
}
